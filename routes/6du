  var userId = req.header('userId'),
    token = req.header('token'),
    sixId = req.header('sixId');
------------------------------------------------------------------------------------------
get  /acceptInvitation
  var userId = req.header('userId'),
    token = req.header('token'),
    code = req.header('code');